ShaderType = Vertex;

void main(){
    float4 myPos = float4(0.0, 0.0, 0.0, 1.0);
    for (uint i = 0; i < 10; i++){
        if (i == 5){
            continue;
        }
        else{
            myPos.x =  myPos.x + 1.0;
        }
    }

    while(myPos.x < 10.0){
        myPos.x = myPos.x + 1.0;
        if (myPos.x == 7.0){
            break;
        }
    }
}